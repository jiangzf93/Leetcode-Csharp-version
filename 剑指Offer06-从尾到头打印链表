public class Solution {
    public int[] ReversePrint(ListNode head) {
        List<ListNode> stack=new List<ListNode>();
        int pointer=0;
        while(head!=null){
            stack.Add(head);
            head=head.next;
            pointer++;
        }
        int[] result=new int[pointer];
        for(int i=pointer-1; i>=0; i--){
            result[pointer-i-1]=stack[i].val;
        }
        return result;
    }
}
