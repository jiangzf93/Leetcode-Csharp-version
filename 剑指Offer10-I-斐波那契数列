public class Solution {
    public int Fib(int n) {
        int numMinusOne=1;
        int numMinusTwo=0;
        int num=0;
        if(n==1){
            num=1;
        }
        for(int i=2;i<=n;i++){
            num=(numMinusOne+numMinusTwo)%1000000007;
            numMinusTwo=numMinusOne;
            numMinusOne=num;
        }
        return num;
    }
}
