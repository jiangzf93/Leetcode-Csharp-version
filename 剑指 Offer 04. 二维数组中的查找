public class Solution {
    public bool FindNumberIn2DArray(int[][] matrix, int target) {
        int rows=matrix.Length;
        if(rows>0){
            int cols=matrix[0].Length;
            int row=0;
            int col=cols-1;
            while(row<=rows-1 && col>=0){
                if(matrix[row][col]==target){
                    return true;
                }else{
                    if(matrix[row][col]>target){
                        col--;
                    }else{
                        row++;
                    }
                }
            }
            return false;
        }else{
            return false;
        }
    }
}
